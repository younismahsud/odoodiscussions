<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
    <data noupdate="1">

        <record id="sequence_openacademy_course" model="ir.sequence">
            <field name="name">Openacademy courses sequence</field>
            <field name="code">openacademy.course</field>
            <field name="prefix">COURSE/%(range_year)s/</field>
            <field eval="1" name="number_next"/>
            <field eval="1" name="number_increment"/>
            <field eval="True" name="use_date_range"/>
            <field eval="False" name="company_id"/>
            <field name="padding">4</field>
        </record>

    </data>
</odoo>